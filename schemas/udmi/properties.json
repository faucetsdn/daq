{
  "title": "Device Properties Schema",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "required": [
    "key_type",
    "version",
    "connect"
  ],
  "properties": {
    "key_type": {
      "enum": [
        "RSA_PEM"
      ]
    },
    "version": {
      "enum": [
        1
      ]
    },
    "connect": {
      "enum": [
        "direct"
      ]
    }
  }  
}
