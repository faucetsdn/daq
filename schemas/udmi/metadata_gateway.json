{
  "title": "Gateway metadata snippet",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "gateway_id": {
      "type": "string",
      "pattern": "^[A-Z]{3}-[1-9][0-9]{0,2}$"
    },
    "protocol": {
      "type": "string"
    },
    "local_id": {
      "type": "string"
    }
  },
  "required": [
    "gateway_id",
    "protocol",
    "local_id"
  ]
}
