{
  "ietf-mud:mud": {
    "mud-version": 1,
    "mud-url": "https://lighting.example.com/lightbulb2000",
    "last-update": "2018-03-02T11:20:51+01:00",
    "cache-validity": 48,
    "is-supported": true,
    "systeminfo": "The BMS IPv4 Example Lightbulb",
    "from-device-policy": {
      "access-lists": {
        "access-list": [
          {
            "name": "mud-76100-fr"
          },
          {
            "name": "bacnet"
          }
        ]
      }
    },
    "to-device-policy": {
      "access-lists": {
        "access-list": [
          {
            "name": "mud-76100-to"
          },
          {
            "name": "bacnet"
          }
        ]
      }
    }
  },
  "ietf-access-control-list:acls": {
    "acl": [
      {
        "name": "mud-76100-to",
        "type": "ipv4-acl-type",
        "aces": {
          "ace": [
            {
              "name": "mqtt-todev",
              "matches": {
                "ipv4": {
                  "ietf-acldns:src-dnsname": "test.com",
                  "protocol": 6
                },
                "tcp": {
                  "ietf-mud:direction-initiated": "from-device",
                  "source-port": {
                    "operator": "eq",
                    "port": 443
                  }
                }
              },
              "actions": {
                "forwarding": "accept"
              }
            },
            {
              "name": "ssh-todev",
              "matches": {
                "ipv4": {
                  "ietf-acldns:src-dnsname": "test.com",
                  "protocol": 6
                },
                "tcp": {
                  "ietf-mud:direction-initiated": "to-device",
                  "destination-port": {
                    "operator": "eq",
                    "port": 23
                  }
                }
              },
              "actions": {
                "forwarding": "accept"
              }
            }
          ]
        }
      },
      {
        "name": "mud-76100-fr",
        "type": "ipv4-acl-type",
        "aces": {
          "ace": [
            {
              "name": "mqtt-frdev",
              "matches": {
                "ipv4": {
                  "ietf-acldns:dst-dnsname": "test.com",
                  "protocol": 6
                },
                "tcp": {
                  "ietf-mud:direction-initiated": "from-device",
                  "destination-port": {
                    "operator": "eq",
                    "port": 443
                  }
                }
              },
              "actions": {
                "forwarding": "accept"
              }
            },
            {
              "name": "ssh-frdev",
              "matches": {
                "ipv4": {
                  "ietf-acldns:dst-dnsname": "test.com",
                  "protocol": 6
                },
                "tcp": {
                  "ietf-mud:direction-initiated": "to-device",
                  "source-port": {
                    "operator": "eq",
                    "port": 23
                  }
                }
              },
              "actions": {
                "forwarding": "accept"
              }
            }
          ]
        }
      },
      {
        "name": "bacnet",
        "type": "ipv4-acl-type",
        "aces": {
          "ace": [
            {
              "name": "bacnet",
              "matches": {
                "ietf-mud:mud": {
                  "controller": "bacnet"
                },
                "ipv4": {
                  "protocol": 17
                },
                "udp": {
                  "source-port": {
                    "operator": "eq",
                    "port": 47808
                  },
                  "destination-port": {
                    "operator": "eq",
                    "port": 47808
                  }
                }
              },
              "actions": {
                "forwarding": "accept"
              }
            }
          ]
        }
      }
    ]
  }
}
