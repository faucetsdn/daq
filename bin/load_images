#!/bin/bash -e

touch .build_built
for var in "$@"; do
    working_dir=$PWD
    cat /tmp/build_artifacts_$var/.build_built >> .build_built
    cd /tmp/build_artifacts_$var/docker_images
    ls -a
    for file in *; do
        docker load < "$file"
    done
    rm -rf /tmp/build_artifacts_$var
    cd $working_dir
done
bin/build_hash write
